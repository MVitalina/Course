<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane cacheShape="false" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.IndexController">
   <children>
      <Label layoutX="77.0" layoutY="32.0" prefHeight="56.0" prefWidth="451.0" text="Вітаємо в бібліотеці! " textAlignment="CENTER">
         <font>
            <Font name="Cambria Italic" size="45.0" />
         </font>
      </Label>
      <TextField fx:id="nickname" focusTraversable="false" layoutX="31.0" layoutY="115.0" prefHeight="56.0" prefWidth="423.0" promptText="Введіть nickname формуляра">
         <font>
            <Font name="Cambria Italic" size="29.0" />
         </font>
      </TextField>
      <Label layoutX="187.0" layoutY="200.0" text="Немає формуляра?">
         <font>
            <Font name="System Italic" size="26.0" />
         </font>
      </Label>
      <Button focusTraversable="false" layoutX="153.0" layoutY="259.0" mnemonicParsing="false" onAction="#NewForm" text="Створити формуляр">
         <font>
            <Font name="System Italic" size="26.0" />
         </font>
      </Button>
      <Label fx:id="isConnected" layoutX="550.0" layoutY="400.0" text="" />
      <Button layoutX="472.0" layoutY="115.0" mnemonicParsing="false" onAction="#Login" prefHeight="56.0" prefWidth="100.0" text="ОК">
         <font>
            <Font name="Cambria Italic" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
